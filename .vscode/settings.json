        "forceToTop": true,
        "blankLinesAfter": 2,
        "license": "Custom"
    },
    "psi-header.changes-tracking": {
        "isActive": true,
        "modAuthor": "Modified By: ",
        "modDate": "Last Modified: ",
        "modDateFormat": "date",
